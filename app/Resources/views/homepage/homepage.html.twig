    </head>

    {% block stylesheet %}
    <link rel="stylesheet" href="{{ asset('build/homepage.css') }}">
    {% endblock %}


    <body>
    <!--Preloader-->
    <div class="loading">
        <div class="svg-wrapper">
            <svg height="200" width="200" xmlns="http://www.w3.org/2000/svg">
                <rect class="animate" height="250" width="250" />
            </svg>
            <div class="text">
                <span class="logo">GRAND ASTHME</span>
                <span class="motto">we trade people health</span>
            </div>
        </div>
    </div>

    <!--SVG animation-->
    <script>
        var p = document.querySelector('.animate'),
            offset = 600;
        var offsetMe = function() {
            if (offset < 0) {
                offset = 600;
            }
            p.style.strokeDashoffset = offset;
            offset = offset - 5;
            requestAnimationFrame(offsetMe);
        }
        offsetMe();
    </script>

    <!--Desktop Navigation-->
    <span class="desktop">
    <header>
      <span id="logo">GRAND ASTHME</span>
      <nav>
        <a href="#">Home</a>
        <a href="#">Calendar</a>
        <a href="#">About</a>
        <a href="#">SMC</a>
      </nav>
    </header>
  </span>

    <!--Mobile Navigation-->
    <span class="mobile">
    <header>
      <span id="logo">GRAND ASTHME</span>
      <div class="button_container">
        <span class="top"></span>
        <span class="middle"></span>
        <span class="bottom"></span>
      </div>
    </header>

    <div class="overlay">
      <nav>
        <ul>
          <li><a href="#">Home</a></li>
          <li><a href="#">Calendar</a></li>
          <li><a href="#">About</a></li>
          <li><a href="#">SMC</a></li>
        </ul>
      </nav>
    </div>
  </span>

    <!--Homepage-->
    <div class="wrapper">
        <div class="landing"></div>
    </div>

    <!--Intro text scroll-->
    <div class="content-1">
        <span class="one">WE TRADE PEOPLE HEALTH</span>
        <span class="two">WE DELIVER CANCER</span>
        <span class="three">WE SELL DEATH</span>
    </div>

    <!--About section-->
    <div class="about">

        <div class="hideme">
            <h1>Choose your Player</h1>
        </div>

        <div class="hideme">
                <div class="row">
                    <div class="col-md-4 offset-md-2>
                        <img src="{{ asset('images/homme_ga.png') }}" alt="">
                    </div>
                    <div class="col-md-4">
                        <img src="{{ asset('images/femme_ga.png') }}" alt="">
                    </div>
                </div>

        </div>

        <!--Contact box-->
        <div class="hideme">
            <div class="container"></div>
            <div class="cube">
                <div class="default-state">
                    <span>Contact  Us</span>
                </div>
                <div class="active-state">
                    <span>blah@blah.com</span>
                </div>
            </div>
        </div>
    </div>

    <!--Sponsors section-->
    <div class="sponsors">
        <div class="hideme">
            <h2>we are greatful to our sponsors</h2>
        </div>
    </div>

    <!--Social section-->
    <div class="social">
    <span class="hideme">
      <a href="#" class="icoTwitter" title="Twitter"><i class="fa fa-twitter"></i></a>
    </span>
    </div>


    </body>

